              <TargetFullRotationsMax>99</TargetFullRotationsMax>
              <TargetAccumulation>Either</TargetAccumulation>
              <TargetPeakFullRotations>2</TargetPeakFullRotations>
            </RotationCondition>
          </Trigger>
          <CGActionWaveFile>
            <Wave>Bilibom</Wave>
            <Pan>0</Pan>
            <Volume>14</Volume>
          </CGActionWaveFile>
        </TriggeredAction>
        <TriggeredAction>
          <Trigger>
            <TriggeringEvent>Yaw0Yaw180</TriggeringEvent>
            <FireLimitPerZero>0</FireLimitPerZero>
