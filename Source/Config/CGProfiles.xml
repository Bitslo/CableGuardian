              <TargetAccumulation>Increasing</TargetAccumulation>
              <TargetPeakFullRotations>0</TargetPeakFullRotations>
            </RotationCondition>
          </Trigger>
          <CGActionWaveFile>
            <Wave>Beep_classic</Wave>
            <Pan>100</Pan>
            <Volume>100</Volume>
          </CGActionWaveFile>
        </TriggeredAction>
      </Actions>
    </Profile>
  </Profiles>
